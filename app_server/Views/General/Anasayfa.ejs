<canvas id="c"></canvas>
<div class="container" id="body-container">
    <div class="row ">
        <div class="col text-center">
            <img src="/static/img/logo.png" />
        </div>
    </div>
    <div class="row ">
        <div class="col text-center">
            <a class="text-white"><i>Everything Around Us</i></a>
        </div>
    </div>

    <div class="row mt-4">
        <div class="col-lg-3 col-md-3 col-sm-1 col-xs"></div>
        <div class="col-lg-6 col-md-6 col-sm-10 col-xs">
            <div class="input-group mt-1">
                <div class="input-group-prepend">
                    <span class="input-group-text"><i class="fab fa-yahoo"></i></span>
                </div>
                <input id="latitude" class="form-control" placeholder="Latitude Enter">
            </div>
            <div class="input-group mt-1">
                <div class="input-group-prepend">
                    <span class="input-group-text" ><i class="fas fa-times"></i></span>
                </div>
                <input id="longitude" class="form-control" placeholder="Longitude Enter">
            </div>
            <div class="input-group mt-1">
                <div class="input-group-prepend">
                    <span class="input-group-text"><i class="fas fa-street-view"></i></span>
                </div>
                <input id="radius" class="form-control" placeholder="Rotate Giriniz">
            </div>
            <button id="SearchButton" class="btn btn-success mt-1 w-100" onclick="SearchNearbyPlace()"><i class="fas fa-search-location">&nbsp;</i>Search Nearby Places</button>
        </div>
        <div class="col-lg-3 col-md-3 col-sm-1 col-xs"></div>
    </div>
</div>

<div class="modal fade bd-example-modal-lg NearbyListModal" tabindex="-1" role="dialog" aria-labelledby="myLargeModalLabel" aria-hidden="true">
  <div class="modal-dialog modal-lg">
    <div class="modal-content">
        <div class="modal-header modal-header-green">
            <h5 class="modal-title" id="exampleModalLongTitle"><i class="far fa-compass">&nbsp;</i>Nearby Places List</h5>
            <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                <span aria-hidden="true">&times;</span>
            </button>
        </div>
        <div class="modal-body">
            <small id="NearbyListShortMessage"></small>
            <ul id="NearbyListComponent" class="list-group">
            </ul>
        </div>
        <div class="modal-footer">
            <small id="NearbyListIsCached"></small>
        </div>
    </div>
  </div>
</div>




